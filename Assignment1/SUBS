def find_substring_locations(s, t):
    locations = []
    i = s.find(t)
    while i != -1:
        locations.append(i + 1)  # 1-based index
        i = s.find(t, i + 1)
    return locations

s = ""
t = "CGCCAAGCG"

locations = find_substring_locations(s, t)
for location in locations:
    print(location)